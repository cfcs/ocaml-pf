(executables
  ((names (parse_conf alcotest_parse))
   (public_names (- -))
   (libraries ( pf alcotest logs.fmt )
   )
))

(alias
 ((name runtest)
  (deps (parse_conf.exe alcotest_parse.exe))
  (action (progn
    (run ./parse_conf.exe ../../../test/freebsd_manpage.pfconf)
    (run ./alcotest_parse.exe)
  ))
 )
)

